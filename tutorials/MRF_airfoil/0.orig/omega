FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

//internalField   uniform 0.125;
internalField   uniform 3.6;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    solid
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    flexible
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    down 
    {
//        type            zeroGradient;
//        type            inletOutlet;
//        inletValue      $internalField;
//        value           $internalField;
       type            slip;
        
    }
    top
    {
//        type            fixedValue;
//        value           $internalField;
//        type            zeroGradient;
       type            slip;
        
    }
    rear
    {
        type           empty;
    }
    front
    {
        type           empty;
    }
    AMIext
    {
        type           cyclicAMI;
    }
    AMI
    {
        type           cyclicAMI;
    }

}


